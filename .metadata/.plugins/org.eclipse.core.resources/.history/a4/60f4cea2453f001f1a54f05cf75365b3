package days09;

/**
 * @author 
 * @date
 * @content
 * @subject
 *
 */
public class Ex03_04 {

	public static void main(String[] args) {
		//[문제]
		//거듭제곱 == 누승 == 멱 == pow
		//2^3 == 밑수를 지수만큼 반복해서 곱하는 것.
		//2^3 == 2*2*2
		
		int result = pow(2,3);
		System.out.println(result);

	}//main
	//일반함수
	private static int pow(int a, int b) {
		int result = 0;
		for (int i = a; i <= b; i++) {
			result = a^b;	
		}
		return result;
	}

}//class
