package days14;

public class Car {

	
		// field
		String name;
		String gearType;
		int door;
		Engine engine;		//has-a 관계
		
		//constructor
		Car(){ }
		
		//method
		void speedUp(int fuel) {
			this.engine.moreFuel(fuel);
		}
		void speedDown(int fuel) {
			this.engine.lessFuel(fuel);
		}
		void stop() {
			this.engine.stop();
		}

	

}
