package days13;

public class Point {

		public int x;
		public int y;
	
		
		public void dispPoint() {
			System.out.printf(">x=%d, y=%d\n", x, y);
		}

		
		//Duplicate method plusPoint(Point) in type Point
		public void plusPoint(Point p) {
			x = x + p.x;
			y = y + p.y;
		}
		
		
		//p1.plusPoint(p1, p2)
		public Point plusPoint(Point p1, Point p2) {
			int x = p1.x + p2.x;
			int y = p1.y + p2.y;
			Point p = new Point();
			p.x = x;
			p.y = y;
			return p;
		}
}
