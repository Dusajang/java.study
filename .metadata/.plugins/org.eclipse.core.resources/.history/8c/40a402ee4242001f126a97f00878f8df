package days11;

import java.util.Arrays;

public class Test01 {

	public static void main(String[] args) {
		//4. 1차원 배열을 2차원 배열로 변환
		
		
		   int [] m = {1,2,3,4,5,6};
		   int [][] n = new int[2][3];    
		   
		  for (int i = 0; i < n.length; i++) {
			for (int j = 0; j < n[0].length; j++) {
				n[i][j] = m[(i*j)+j];
			}
		}
		   System.out.println(Arrays.toString(n));
		   
		   
		   /*
		   int[][] n = {
					{1,2,3},
					{4,5,6}
				};
			*/


		
	}//main

	

}//class
