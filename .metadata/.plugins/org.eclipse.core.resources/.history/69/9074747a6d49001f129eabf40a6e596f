public class Ex03_03 {

	public static void main(String[] args) {
		LocalDate ld = LocalDate.now();
		System.out.println(ld);  //2024-07-24
		//년
		//int year =  ld.get(ChronoField.YEAR);
		int year = ld.getYear();
		System.out.println(year);
		//월
		//일

	}//main

}//class
