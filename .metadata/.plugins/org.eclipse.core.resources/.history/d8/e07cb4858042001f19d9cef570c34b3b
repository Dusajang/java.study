public class Ex05 {

	public static void main(String[] args) {
		//로또 게임 + (게임 횟수 입력)
		int n = 5;
		//int[] lotto = new int[6*5];
		int[][] lotto = new int[n][6];
		
		filllotto();
		System.out.println(Arrays.deepToString(lotto));
		
		

	}//main

	private static void filllotto() {
		int n = 5;
		int[][] lotto = new int[n][6];
		int lottoNub = (int)((Math.random()*45))+1;
		for (int i = 0; i < n-1; i++) {
			for (int j = 0; j < lotto[0].length-1; j++) {
			lotto[i][j] = (int)((Math.random()*45))+1;
			}
		}
		
	}//filllotto()

}//class





