public class Ex05 {

	public static void main(String[] args) {
		//로또 게임 + (게임 횟수 입력)
		int n = 5;
		//int[] lotto = new int[6*5];
		int[][] lotto = new int[n][6];
		
		filllotto();
		System.out.println(Arrays.toString(lotto));
		
		

	}//main

	private static void filllotto() {
		int n = 5;
		int[][] lotto = new int[n][6];
		int lottoNub = (int)((Math.random()*45))+1;
		for (int i = 0; i < lotto[0].length; i++) {
			lotto[n][i] = (int)((Math.random()*45))+1;
		}
		
	}

}//class





