public class Ex03 {

	public static void main(String[] args) {
		//객체(클래스) 복사와 복제
		//생성자 복제
		Point p1 = new Point(1,2);
		
		Point p2 = new Point(p1);

	}//main

}//class


class Point {
	int x, y;
	Point(){
		this(0,0);
	}
	Point(int x, int y){
		this.x = x;
		this.y = y;
	}
	
	public Point(Point p) {
		this.x = x;
		this.y = y;
	}
	String getXY() {
		return String.format("(%d, %d)", this.x, this.y);
	}
}









