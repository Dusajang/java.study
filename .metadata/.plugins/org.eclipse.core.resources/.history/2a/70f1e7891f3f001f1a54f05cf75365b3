package days09;

public class Test02 {

	public static void main(String[] args) {
		//2. 로또645 에서  로또 번호 중복 체크하는 메서드만 선언하세요
		   //isDuplicateLotto()
		
		int [] lotto = new int[6];
		
		isDuplicateLotto()
		
		

	

		
		
	}//main
	
	private static boolean isDuplicateLotto(int [] lotto, int lottoNumber, int index) {
		for (int i= 0;  i< index; i++) {
			if(lotto[i] == lottoNumber) {
				return false;
			}
		}
	}
	
	
	
}//class
