public class Ex05_04 {

	public static void main(String[] args) {
		//int[] kor = new int[270]
		//3학년 2반 17번 학생의 국어
		//kor[226]
		//int[][][] kor = new int [3][3][30]
		//kot[2][1][16]
		
		
		
		//int[] m = new int[24]
		//int[][] m = new int [4][6]
		int[][][] m = new int [2][3][4];     //2면 3행 4열
		
		System.out.println(m.length);		//3차원에선 면 갯수
		
		System.out.println(m[0].length);		//3차원에선 면 갯수
		System.out.println(m[1].length);		//3차원에선 면 갯수
		
		

	}//main

}//class







