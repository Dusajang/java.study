package days17;

import java.util.Calendar;

public class Ex09 {

	public static void main(String[] args) {
		//jdk 1.0    java.util.Date 클래스
		//                        ㄴjava.sql.Date
		
		//jdk 1.1    java.util.Calendar 클래스
		
		Calendar c = Calendar.getInstance();    // 그레고리력에서 업캐스팅
		System.out.println(c.toString());

	}

}
