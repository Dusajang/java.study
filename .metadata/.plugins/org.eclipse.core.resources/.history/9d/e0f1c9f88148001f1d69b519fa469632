package days17;

public class Test01 {

	public static void main(String[] args) {
		//출력값 "ababababab";
		String str1 = "aaaaa";
		String str2 = "bbbbb";
		String answer = "";
		
		for (int i = 0; i < 10; i++) {
			answer += str1.charAt(i);
			answer += str2.charAt(i);
		}
		System.out.println(answer);
        //return answer;

	}//main

}//class
